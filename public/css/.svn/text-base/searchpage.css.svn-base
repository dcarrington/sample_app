#searchColumns {
	background: url(../images/searchColumns.gif);
	height: 1%;
}

#searchColumns #navigation li ul li {
	list-style-type: disc;
}

#searchColumns {
	float: left;
	width: 200px;
	padding: 0;
	display: inline;
	margin: 0 0 0 0px;
	font-size: 90%;
}

#disclaimer {
	margin: 0 20px 20px 20px;
}

#search {
	margin: 10px 20px 20px 220px;
	border: 1px solid #ccc;
	padding: 1px 1px 10px;
	border: 1px solid #57578F;
	background: #DFDFDF;
}

#search h2 {
	margin: 0;
	padding: 4px 4px;
	background: #57578F;
	color: #fff;
}

#search ul {
	list-style-type: disc;
	margin-top: 10px;
}
