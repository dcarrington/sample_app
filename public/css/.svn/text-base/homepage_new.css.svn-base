#online_service {
	width: 230px;
	float: center;
	display: inline;
	margin: 10px 0 0 10px;
}

#online_service form {
	background: #FFFFE1;
	border: 1px solid #000;
}

#online_service form .message {
	background: #fff;
	border: none;
	border-bottom: 1px solid #ddd;
	margin: 0 -4px 0 -6px;
	height: 3.6em;
}

#online_service form h2 {
	margin: 0;
	clear: both;
	padding: 4px 4px;
	background: #4C4C82;
	color: #FFFFE1;
}

#homepage_login {
	background: #E7E5E2;
	font-size: 90%;
}

#homepage_login form {
	background: #E7E5E2;
}

#search {
	float: center;
	display: inline;
	margin: 15em 0 0 10px;
	padding: 1px 1px 10px;
}

#search h2 {
	margin: 0;
	clear: both;
	padding: 4px 4px;
	background: #4C4C82;
	color: #FFFFE1;
}

#search form{
	background: #DAE0D2;
	font-size: 90%;
}

#search form h3 {
	margin: 0;
	padding: 4px 4px;
	background: #DAE0D2;
	color: #57578F;
}

#search form input {
	font-size: 90%;
}

.whatsNew {
	float: left;
	margin: 10px 0 0 10px;
	padding: 1px 1px 10px;
	font-size: 90%;
}

.whatsNew h2 {
	margin: 0;
	padding: 0;
	color: #57578F;
}

.whatsNew a {
	color: #765;
}

.whatsNew a:hover {
	color: #333;
}

.whatsNew ul {
	list-style-type: disc;
	padding-left: 10px;
	margin-top: 10px;
}

.links {
	float: left;
	margin: 5px 0 0 5px;
	padding: 1px 1px 5px;
	font-size: 90%;
}

.links h2, h3 {
	margin: 0;
	padding: 0;
	color: #000;
}

.links a {
	color: #765;
}

.links a:hover {
	color: #333;
}

.links ul {
	list-style-type: disc;
	padding-left: 5px;
	margin-top: 0;
}

.links ul li {
	list-style-type: disc;
	padding-left: 0;
	margin-top: 0;
}

#disclaimer {
	margin: 0 20px 20px 20px;
}

#generalInfo {
	margin: 0 20px 20px 20px;
}

#layout .leftcol, .rightcol {
	background: #f4f4f4;
	width: 25%;
	vertical-align: top;
}

#layout .middlecol {
	width: 50%;
	vertical-align: top;
}

.moc ul li a{
	background: none;
}

#layout img {
	width: 235;
	height: 200;
}

#homelayout {
	margin: 0 20px 20px 20px;
}

#homelayout img {
	width: 17em;
	height: 10em;
}